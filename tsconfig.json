{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": false,
    "target": "ES2018",
    "lib": [
      "es2018"
    ],
    "outDir": "./src",
    "baseUrl": "./_src",
    "strict": true,
    "esModuleInterop": true
  },
  "sourceMap": true,
  "include": [
    "_src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "_lib",
    "lib"
  ]
}
