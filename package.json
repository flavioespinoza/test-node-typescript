{
  "name": "test-node-typescript",
  "version": "1.0.0",
  "description": "Node v10 typescript test driven web app",
  "main": "lib/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "ts-node ./lib/server.ts",
    "devts": "ts-node-dev --respawn --transpileOnly ./lib/app.ts",
    "start": "node ./src/server.js",
    "mon": "nodemon ./src/server.js",
    "prod": "npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com/flavioespinoza/test-node-typescript.git"
  },
  "keywords": [
    "Node",
    "v10",
    "typescript",
    "test",
    "asyc",
    "await",
    "es6"
  ],
  "author": "Flavio Espinoza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flavioespinoza/test-node-typescript/issues"
  },
  "homepage": "https://github.com/flavioespinoza/test-node-typescript#readme",
  "dependencies": {
    "@flavioespinoza/log_log": "^1.0.52",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.122",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chance": "^1.0.16",
    "docdown": "^0.7.3",
    "express": "^4.16.4",
    "lodash": "^4.17.10",
    "markdown-doctest": "^0.9.1",
    "ololog": "^1.1.103",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {}
}
